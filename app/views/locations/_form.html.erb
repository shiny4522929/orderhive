<%= form_with(model: location) do |form| %>
  <div>
    <%= form.label :name_of_location %><br>
    <%= form.text_field :name_of_location %>
  </div>

  <div>
    <%= form.label :address %><br>
    <%= form.text_field :address %><br>
  </div>

  <div>
    <%= form.label :location_identification_code %><br>
    <%= form.text_field :location_identification_code %><br>
  </div>
   
  <div>
    <%= form.label :postal %><br>
    <%= form.text_field :postal %><br>
  </div>

  <div>
    <%= form.label :code %><br>
    <%= form.text_field :code %><br>
  </div>

  <div>
    <%= form.label :state %><br>
    <%= form.text_field :state %><br>
  </div>

  <div>
    <%= form.label :country %><br>
    <%= form.text_field :country %><br>
  </div>

  <div>
    <%= form.label :user_id %><br>
    <%= form.text_field :user_id %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
<% render "edit", edit_location_path(@location) %>
  